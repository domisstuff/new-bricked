{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "functions": {
    "api/publish.js": {
      "maxDuration": 10
    },
    "api/serve.js": {
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/api/publish",
      "destination": "/api/publish.js"
    },
    {
      "source": "/:username",
      "destination": "/api/serve?username=:username"
    },
    {
      "source": "/api/delete",
      "destination": "/api/delete.js"
    },
    {
      "source": "/api/players",
      "destination": "/api/players.js"
    }
  ]
}
