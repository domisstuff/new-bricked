{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "functions": {
    "api/publish.js": {
      "maxDuration": 10
    },
    "api/serve.js": {
      "maxDuration": 5
    },
    "api/false.js": {
      "maxDuration": 5
    },
    "api/echo.js": {
      "maxDuration": 5
    },
    "api/true.js": {
      "maxDuration": 5
    },
    "api/battery/connection/get.js": {
      "maxDuration": 5
    },
    "api/battery/connection/post.js": {
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/api/publish",
      "destination": "/api/publish.js"
    },
    {
      "source": "/:username",
      "destination": "/api/serve?username=:username"
    },
    {
      "source": "/api/false",
      "destination": "/api/false.js"
    },
    {
      "source": "/api/echo",
      "destination": "/api/echo.js"
    },
    {
      "source": "/api/true",
      "destination": "/api/true.js"
    },
    {
      "source": "/api/battery/connection/get",
      "destination": "/api/battery/connection/get.js"
    },
    {
      "source": "/api/battery/connection/post",
      "destination": "/api/battery/connection/post.js"
    }
  ]
}
